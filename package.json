  "version": "0.0.1",
  "packageManager": "pnpm@7.18.1",
  "description": "",
  "author": "ocavue <ocavue@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/ocavue/remirror-extension-flat-list#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ocavue/remirror-extension-flat-list.git"
  },
  "bugs": "https://github.com/ocavue/remirror-extension-flat-list/issues",
  "keywords": [],
  "sideEffects": false,
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/style.css",
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "run-p dev:tsup dev:playground",
    "build": "run-s build:tsup build:playground",
    "dev:tsup": "tsup --watch",
    "dev:playground": "nr --filter ./playground dev",
    "build:tsup": "tsup",
    "build:playground": "nr --filter ./playground build",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.6",
    "@remirror/core": "^2.0.9",
    "@remirror/extension-events": "^2.1.9"
