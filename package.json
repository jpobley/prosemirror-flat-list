  "devDependencies": {
    "@antfu/ni": "^0.18.8",
    "@ocavue/eslint-config": "^0.4.5",
    "@remirror/pm": "^2.0.2",
    "@types/node": "^18.11.15",
    "eslint": "^8.29.0",
    "esno": "^0.16.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-nesting": "^10.2.0",
    "prettier": "^2.8.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.1",
    "vitest": "^0.25.8"
  }
